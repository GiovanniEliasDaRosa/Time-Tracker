{
  "manifest_version": 2,
  "name": "Time Tracker",
  "version": "1",
  "description": "Tracks time spent on sites",
  "permissions": ["tabs", "storage"],
  "icons": {
    "48": "img/icon-48.png",
    "64": "img/icon.png"
  },
  "background": {
    "scripts": [
      "js/Storage.js",
      "js/TabManager.js",
      "js/functions.js",
      "js/background.js",
      "js/bootstrap.js"
    ],
    "persistent": true
  }
}
