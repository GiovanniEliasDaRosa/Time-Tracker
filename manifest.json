{
  "manifest_version": 2,
  "name": "Time Tracker",
  "version": "1",
  "description": "Tracks time spent on sites",
  "permissions": ["storage"],
  "icons": {
    "48": "img/icon-48.png",
    "64": "img/icon.png"
  },
  "background": {
    "scripts": ["js/Storage.js", "js/functions.js", "js/bootstrap.js", "js/background.js"],
    "persistent": false
  }
}
